{
  "env": {
    "test": {
      "plugins": [
        "@babel/plugin-transform-modules-commonjs",
        "babel-plugin-transform-import-meta"
      ]
    },
    "production": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
      ]
    },
    "development": {
      "presets": [
        ["@babel/preset-react", { "development": true }]
      ]
    }
  }
}
